<% if(data.message == '0'){ %>
  <button class="form-control" id="test" onclick="myAlertTop_success()" style="display: none;"><%= data.message %></button>
  <% } else if(data.message == '1') {%>
    <button class="form-control" id="test" onclick="myAlertTop_warning()" style="display: none;"><%= data.message %></button>
    <% } %>


      <script>
        var test1 = '<%= data.message %>';
        if (test1 != '') {
          setTimeout(function() {
            document.getElementById('test').click();
          }, 100);
        }
      </script>

      <script>
        function confirmAction() {
          return confirm("Bạn có chắc sẽ thực hiện yêu cầu này?");
        }
      </script>
      <script>
        iziToast.settings({
          timeout: 3000,
          resetOnHover: true,
          transitionIn: 'flipInX',
          transitionOut: 'flipOutX',
          position: 'bottomRight',
          theme: 'light',
        });
      </script>
      <script>
        function myAlertTop_success() {
          iziToast.success({
            title: 'Thành Công!',
            message: '<%= data.message1%>',
          });
        }
      </script>
      <script>
        function myAlertTop_warning() {
          iziToast.error({
            title: 'Thất bại!',
            message: '<%= data.message1%>',
          });
        }
      </script>

